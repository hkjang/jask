# Database
DATABASE_URL="postgresql://jask:jask_password@localhost:5432/jask?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=1d

# LLM Providers
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=gwen3:8b
EMBEDDING_MODEL=bona/bge-m3-korean

VLLM_BASE_URL=http://localhost:8000
VLLM_MODEL=codellama/CodeLlama-7b-Instruct-hf

# Development Mode (set to true if Ollama is not available)
USE_MOCK_LLM=false

# Default LLM Provider (ollama or vllm)
DEFAULT_LLM_PROVIDER=ollama

# SQL Execution Limits
SQL_MAX_ROWS=1000
SQL_TIMEOUT_MS=30000
SQL_ALLOW_WRITES=false

# Server
PORT=4000
